    port:
    = @coin.rpc_port
  %p 
    difficulty_method:
    = @coin.rpc_difficulty
%p
  %b Difficulty:
  = @coin.difficulty
%p
  %b Update date:
  = @coin.update_date

= link_to 'Edit', edit_coin_path(@coin)
