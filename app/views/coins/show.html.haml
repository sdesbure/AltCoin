%p#notice= notice

%p
  %b Name:
  = @coin.name
%p
  %b Algorithm:
  = @coin.algorithm
%p
  %b Tag:
  = @coin.tag
%p
  %b RPC:
  %p 
    user:
    = @coin.rpc_user
  %p 
    password:
    = @coin.rpc_password
  %p 
    ip address:
    = @coin.rpc_url
